POSTGRES_PORT=5432
POSTGRES_DATABASE=ecommerce_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123

# Consumer Configuration
BATCH_SIZE=100
BATCH_TIMEOUT_MS=5000
LOG_LEVEL=INFO
